#include <stdio.h>
// volume of a cylinder

int main()
{
    const pi=3.142;
    int radius;
    int height;
    int volume;

    printf("enter the radius");
    scanf("%d",&radius);

    printf("enter the height");
    scanf("%d",&height);
     volume =pi*radius*radius*height;

      printf("the volume is%d ",volume);






    return 0;
}
